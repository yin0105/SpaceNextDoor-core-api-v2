{
  "name": "snd-api",
  "version": "0.0.1",
  "description": "Space Next Door - Core API",
  "author": "Muhammad Junaid",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/Space-Next-Door/core-api-v2",
  "bugs": {
    "url": "https://github.com/Space-Next-Door/core-api-v2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Space-Next-Door/core-api-v2"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:export": "eslint . --ext .ts -f checkstyle -o checkstyle-result.xml",
    "lint:fix": "eslint --fix . --ext .ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "jest --testTimeout=10000 ",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gen:typings": "ts-node src/generate-typings.ts",
    "db:migrate": "npx sequelize-cli db:migrate",
    "start-cron:create-payouts:dev": "ts-node src/cron-job/create-payouts/create-payouts.cron.ts",
    "start-cron:create-payouts:prod": "node dist/src/cron-job/create-payouts/create-payouts.cron.js",
    "start-cron:pay-renewal:dev": "ts-node src/cron-job/pay-renewals/pay-renewal.cron.ts",
    "start-cron:pay-renewal:prod": "node dist/src/cron-job/pay-renewals/pay-renewal.cron.js",
    "start-cron:cancel-booking:dev": "ts-node src/cron-job/cancel-bookings/cancel-booking.cron.ts",
    "start-cron:cancel-booking:prod": "node dist/src/cron-job/cancel-bookings/cancel-booking.cron.js",
    "start-cron:sitelink-stock-sync:dev": "ts-node src/cron-job/sitelink-stock-sync/sitelink-sync.cron.ts",
    "start-cron:sitelink-stock-sync:prod": "node dist/src/cron-job/sitelink-stock-sync/sitelink-sync.cron.js",
    "start-cron:active-booking:dev": "ts-node src/cron-job/active-bookings/active-bookings.cron.ts",
    "start-cron:active-booking:prod": "node dist/src/cron-job/active-bookings/active-bookings.cron.js",
    "start-cron:terminate-booking:dev": "ts-node src/cron-job/termination/terminate-bookings.cron.ts",
    "start-cron:terminate-booking:prod": "node dist/src/cron-job/termination/terminate-bookings.cron.js",
    "start-cron:refund-deposit-booking:dev": "ts-node src/cron-job/refund-deposit/refund-deposit.cron.ts",
    "start-cron:refund-deposit-booking:prod": "node dist/src/cron-job/refund-deposit/refund-deposit.cron.js",
    "start-cron:es-stock-sync:dev": "ts-node src/cron-job/es-stock-sync/es-stock-sync.cron.ts",
    "start-cron:es-stock-sync:prod": "node dist/src/cron-job/es-stock-sync/es-stock-sync.cron.js",
    "start-cron:quotation-reject:dev": "ts-node src/cron-job/quotations/quotation-reject.cron.ts",
    "start-cron:quotation-reject:prod": "node dist/src/cron-job/quotations/quotation-reject.cron.js",
    "start-cron:quotation-reminder:dev": "ts-node src/cron-job/quotations/quotation-reminder.cron.ts",
    "start-cron:quotation-reminder:prod": "node dist/src/cron-job/quotations/quotation-reminder.cron.js"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "@golevelup/nestjs-rabbitmq": "^1.17.1",
    "@nestjs/common": "^7.6.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.1",
    "@nestjs/graphql": "^7.9.1",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.1",
    "@nestjs/platform-fastify": "^7.5.1",
    "@nestjs/terminus": "^7.0.0",
    "@sendgrid/mail": "^7.4.1",
    "@types/stripe": "^8.0.417",
    "amqp-connection-manager": "^3.2.2",
    "apollo-server-core": "^2.19.0",
    "apollo-server-express": "^2.19.0",
    "apollo-server-fastify": "^2.19.0",
    "await-to-js": "^2.1.1",
    "aws-sdk": "^2.821.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.1",
    "clevertap": "^1.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.9.7",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "graphql-type-json": "^0.3.2",
    "http-json-errors": "^1.2.8",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mime-types": "^2.1.28",
    "nestjs-stripe": "^0.4.0",
    "otp-generator": "^2.0.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.2.1",
    "phone": "^2.4.21",
    "puppeteer": "^7.0.1",
    "query-string": "^6.13.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sequelize": "^5.21.3",
    "sequelize-replace-enum-postgres": "^1.6.0",
    "sequelize-typescript": "^1.1.0",
    "sharp": "^0.27.0",
    "stripe": "^8.132.0",
    "twilio": "^3.54.1",
    "type-graphql": "^1.0.0-rc.3",
    "user-agents": "^1.0.634",
    "uuid": "^8.3.2",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.1.1",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.6",
    "@types/passport-google-oauth20": "^2.0.6",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.9",
    "@types/user-agents": "^1.0.1",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "bluebird": "^3.7.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "4.3.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "rxjs-tslint": "^0.1.8",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      ".module.ts",
      ".validation.ts",
      "<rootDir>/dispatcher.ts",
      "<rootDir>/generate-typings.ts",
      "<rootDir>/graphql.schema.ts",
      "<rootDir>/main.ts",
      "<rootDir>/src/app/main.ts",
      ".seed.ts",
      "<rootDir>/config/",
      "<rootDir>/db/",
      "<rootDir>/shared/"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 3,
        "functions": 5,
        "lines": 25,
        "statements": 25
      }
    }
  }
}
